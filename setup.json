{
    "name": "aiida-unicore",
    "author": "FZJ-JSC",
    "author_email": "c.witzler@fz-juelich.de",
    "description": "Aiida plugin that allows to schedule jobs and transport files using unicore",
    "url": "https://github.com/FZJ-JSC/aiida-unicore",
    "license": "MIT",
    "classifiers": [
        "Programming Language :: Python",
        "Intended Audience :: Science/Research",
        "License :: OSI Approved :: MIT License",
        "Natural Language :: English",
        "Framework :: AiiDA"
    ],
    "version": "0.1.0a0",
    "entry_points": {
        "aiida.transports": [
            "unicore = aiida_unicore.transports:UnicoreTransport"
        ],
        "aiida.schedulers": [
            "unicore = aiida_unicore.schedulers:Unicore"
        ],
        "aiida.cmdline.data": [
            "unicore = aiida_unicore.cli:data_cli"
        ]
    },
    "include_package_data": true,
    "setup_requires": ["reentry"],
    "reentry_register": true,
    "install_requires": [
        "aiida-core>=1.1.0,<2.0.0",
        "six",
        "voluptuous"
    ],
    "extras_require": {
        "testing": [
            "pgtest~=1.3.1",
            "wheel~=0.31",
            "coverage",
            "pytest~=3.6,<5.0.0",
            "pytest-cov"
        ],
        "pre-commit": [
            "astroid==2.2.5",
            "pre-commit==1.20.0",
            "prospector==1.1.7",
            "pylint==2.3.1"
        ],
        "docs": [
            "sphinx",
            "sphinxcontrib-contentui",
            "sphinxcontrib-details-directive; python_version>='3.0'",
            "sphinx-rtd-theme"
        ]
    }
}
